<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ssm.mapper.GcuserMapper">

    <select id="findGcuser" resultType="gcuser" >
        select * 
        	from gc_user		
    </select> 
    
    <select id="LoginGcuser" resultType="gcuser" parameterType="String" >
        select * 
        	from gc_user where uTel=#{uTel}	
    </select> 
    
    
</mapper>